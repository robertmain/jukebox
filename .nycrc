{
    "watermarks": {
        "lines": [80, 95],
        "functions": [80, 95],
        "branches": [80, 95],
        "statements": [80, 95]
    },
    "reporter": [
        "json",
        "text-summary"
    ],
    "extension": [
        ".ts"
    ],
    "exclude": [
        "test/**/*.*",
        "src/example.ts"
    ],
    "cache": false,
    "all": true
}