{
  "name": "jukebox-backend",
  "version": "1.0.0",
  "description": "Backend for node jukebox",
  "main": "dist/index.js",
  "scripts": {
    "test": "nyc --all _mocha --require ts-node/register -R spec test/**/*.ts",
    "__coverage": "rm -rf coverage && nyc report",
    "coverage:html": "npm run __coverage -- --reporter=html",
    "coverage:json": "npm run __coverage -- --reporter=json",
    "codecov": "npm run coverage:json && codecov -f coverage/*.json",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "start": "npm run build && node $npm_package_main",
    "prepublish": "npm run build"
  },
  "author": "Robert Main",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:robertmain/jukebox.git"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/fluent-ffmpeg": "^2.1.6",
    "@types/node": "^10.3.1",
    "@types/socket.io": "^1.4.34",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "fluent-ffmpeg": "^2.1.2",
    "inversify": "^4.13.0",
    "inversify-express-utils": "^4.2.2",
    "jayson": "^2.0.6",
    "redux": "^3.7.2",
    "reflect-metadata": "^0.1.12",
    "socket.io": "^2.1.1",
    "speaker": "^0.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/istanbul": "^0.4.30",
    "@types/mocha": "^5.2.1",
    "@types/mock-require": "^2.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mock-require": "^3.0.2",
    "nyc": "^12.0.2",
    "ts-node": "^6.1.0",
    "typemoq": "^2.1.0",
    "typescript": "^2.9.1"
  },
  "engines": {
    "node": ">=6"
  }
}
