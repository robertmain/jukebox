{
  "name": "jukebox-backend",
  "version": "1.0.0",
  "description": "Backend for node jukebox",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test": "npm run unit && npm run e2e",
    "unit": "jest",
    "e2e": "jest --config ./test/e2e/jest.json",
    "webpack": "webpack --config webpack.config.js",
    "lint": "eslint src/**/*.ts",
    "docs:code": "typedoc --out docs/code src/"
  },
  "author": "Robert Main",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:robertmain/jukebox.git"
  },
  "dependencies": {
    "@nestjs/common": "^5.7.4",
    "@nestjs/core": "^5.7.4",
    "@types/fluent-ffmpeg": "^2.1.11",
    "fastify-formbody": "^3.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "speaker": "^0.4.1",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.7.4",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.3",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "codecov": "^3.6.1",
    "eslint": "^5.16.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^21.2.4",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.15.3",
    "typemoq": "^2.1.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
