{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for node jukebox",
  "main": "dist/index.js",
  "scripts": {
    "test": "nyc --all _mocha --require ts-node/register -R spec test/**/*.ts",
    "__coverage": "rm -rf coverage && nyc report",
    "coverage:html": "npm run __coverage -- --reporter=html",
    "coverage:json": "npm run __coverage -- --reporter=json",
    "codecov": "npm run coverage:json && codecov -f coverage/*.json",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "start": "node $npm_package_main",
    "prepublish": "npm run build"
  },
  "author": "Robert Main",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:robertmain/jukebox.git"
  },
  "dependencies": {
    "@types/express": "^4.0.37",
    "@types/fluent-ffmpeg": "^2.1.4",
    "@types/node": "^8.0.33",
    "@types/sql.js": "0.0.29",
    "express": "^4.16.2",
    "express-jsonschema": "^1.1.6",
    "fluent-ffmpeg": "^2.1.2",
    "inversify": "^4.3.0",
    "inversify-express-utils": "^4.0.1",
    "reflect-metadata": "^0.1.10",
    "socket.io": "^2.0.3",
    "speaker": "^0.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/istanbul": "^0.4.29",
    "@types/mocha": "^2.2.43",
    "@types/mock-require": "^1.3.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^2.3.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "mock-require": "^2.0.2",
    "nyc": "^11.2.1",
    "ts-node": "^3.3.0",
    "typemoq": "^2.0.1",
    "typescript": "^2.5.3"
  }
}
