{
  "name": "jukebox-backend",
  "version": "1.0.0",
  "description": "Backend for node jukebox",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test": "npm run unit && npm run e2e",
    "unit": "jest",
    "e2e": "jest --config ./test/e2e/jest.json",
    "webpack": "webpack --config webpack.config.js",
    "lint": "tslint -c tslint.json test/unit/**/*.ts src/**/*.ts",
    "docs:code": "typedoc --out docs/code src/"
  },
  "author": "Robert Main",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:robertmain/jukebox.git"
  },
  "dependencies": {
    "@nestjs/common": "^5.7.3",
    "@nestjs/core": "^5.6.2",
    "@types/fluent-ffmpeg": "^2.1.9",
    "fastify-formbody": "^3.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "speaker": "^0.4.1",
    "typescript": "^3.4.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.7.3",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.9",
    "@types/node": "^11.9.6",
    "@types/supertest": "^2.0.7",
    "codecov": "^3.1.0",
    "jest": "^23.6.0",
    "prettier": "^1.16.4",
    "supertest": "^3.4.2",
    "ts-jest": "^21.2.4",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.13.1",
    "typedoc": "^0.14.2",
    "typemoq": "^2.1.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
